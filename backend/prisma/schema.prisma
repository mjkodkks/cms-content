// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id          String   @id @default(uuid())
  username    String   @unique
  displayName String
  imageUrl    String?
  createdAt   DateTime @default(now())
}

model Blog {
  id        String   @id @default(uuid())
  title     String
  content   String?
  published Boolean? @default(false)
  author    String?
  authorId  Int?
  SEO       SEO?     @relation(fields: [seoId], references: [id])
  seoId     String?
}

model SEO {
  id          String @id @default(uuid())
  tilte       String
  description String
  keyword     String
  Blog        Blog[]
}

model Config {
  id             String  @id @default(uuid())
  title_tag      String?
  description    String?
  google_keyword String?
}

